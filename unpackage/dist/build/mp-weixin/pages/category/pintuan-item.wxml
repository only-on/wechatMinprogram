<block wx:if="{{options.title}}"><view class="media-item goods-list view" hover-class="media-item-hover" data-event-opts="{{[['tap',[['click',['$event']]]]]}}" bindtap="__e"><view class="image"><image src="{{options.url}}"></image></view><view class="content"><view class="title">{{options.title}}</view><block wx:if="{{options.minAskType==1}}"><text style="margin-right:15rpx;color:#FC4D52;font-size:30rpx;">{{options.minAsk+"件成团"}}</text></block><block wx:if="{{options.minAskType==0}}"><text style="margin-right:15rpx;color:#FC4D52;font-size:30rpx;">{{options.minAsk+"人成团"}}</text></block><view class="num s-row"><block wx:for="{{options.labelNames}}" wx:for-item="itema1" wx:for-index="indexa1" wx:key="indexa1"><block wx:if="{{indexa1<5}}"><text class="tmprice">{{itema1}}</text></block></block></view><view class="money s-row"><text class="coupon-price"><text>{{"¥"+options.discountPrice}}</text><text class="del">{{"¥"+options.originalPrice}}</text></text></view></view><view class="media-item-line" style="position:absolute;"></view></view></block>

page {
	background-color: #F7F7F7;
}
.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	width: 100%;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.yt-list-cell {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 15rpx 40rpx 15rpx 40rpx;
	line-height: 70rpx;
	width: 100%;
}
.cell-tit {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	font-size: 26rpx;
	color: $font-color-light;
	margin-right: 10rpx;
}
.desc {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	font-size: $font-base;
	color: $font-color-dark;
	text-align: right;
}


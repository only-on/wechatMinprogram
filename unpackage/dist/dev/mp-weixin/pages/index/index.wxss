.index-content {
	width: 100%;
	background: #fff;
}
.index-content .index-header {
	position: fixed;
	top: 0;
	z-index: 160;
	border-bottom: solid 1px #ddd;
	/* background: linear-gradient(to left, #FA4DBE 0, #FBAA58 100%); */
	background-color: #49B76D;
	/* padding-top: 50px; */
	border-bottom-color: transparent;
	-webkit-transition: all .4s ease 0s;
	-webkit-transform-origin: center;
	        transform-origin: center;
	
	width: 100%;
}
.index-content .index-header .icon_header {
	width: 100%;
	line-height: 45px;
	position: relative;
}
.index-content .index-header .icon_header .index-search {
	text-align: center;
	font-size: 16px;
	color: #F7F7F7;
	position: relative;
	left: 65px;
	width: 60%;
	z-index: 2;
	zoom: 1;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	padding-top: 8px;
	margin: 0 45px 0 10px;
	padding-bottom: 6px;
}
.index-content .index-header .icon_header .index-search .icon_search {
	background: #F7F7F7;
	border-radius: 40rpx;
	-moz-border-radius: 40rpx;
	-webkit-border-radius: 40rpx;
	-o-border-radius: 40rpx;
	-ms-border-radius: 40rpx;
	height: 60rpx;
	line-height: 56rpx;
	font-size: 28rpx;
	color: #ccc;
	text-align: left;
	text-indent: 32rpx;
	position: relative;
	z-index: 1;
	zoom: 1;
	transition: all .4s ease 0s;
	-o-transition: all .4s ease 0s;
	-moz-transition: all .4s ease 0s;
	-webkit-transition: all .4s ease 0s;
	-webkit-transform-origin: center;
	        transform-origin: center;
}
.icon_header .icon_search>.iconfont {
	margin-right: 20rpx;
	top: 1px;
	color: #ccc;
}
.icon_header .icon_suji {
	position: absolute;
	bottom: 0;
	width: 160rpx;
	text-align: center;
	left: 0;
	color: #FFFFFF;
}
.icon_header .icon_suji image {
	width: 60rpx;
	height: 60rpx;
	vertical-align: middle;
	position: absolute;
	top: -60%;
}
.icon_header .icon_suji .iconfont {
	font-size: 26rpx;
}
.icon_header .icon_suji .icon-zuji {
	font-size: 40rpx;
}
.index-content .index-banner {
	margin-top: 50rpx;
	width: 100%;
	height: 450rpx;
}
.index-content .index-banner swiper-item {
	height: 450rpx;
}
.index-content .index-banner .swiper .swiper-container {
	height: 450rpx;
}
.index-content .index-banner .swiper .swiper-container image {
	width: 100%;
}
.index-content .index-navlist {
	border-bottom: 20rpx solid #f2f2f2
}
.index-content .index-navlist .scroll {
	width: 100%;
	overflow: hidden;
	white-space: nowrap;
}
.index-content .index-navlist .scroll .scin{
	display: inline-block;
	float: none;
}
.index-content .index-navlist image {
	width: 54px;
	height: 54px;
	border-radius: 10rpx;
}
